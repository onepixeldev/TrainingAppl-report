	<subDataset name="STAFF_LIST" uuid="4488657b-5f6e-4382-b385-2ae43e87d0a5">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MyHRIS"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="P_STH_STATUS" class="java.lang.String"/>
		<parameter name="P_TH_DATE_FROM" class="java.lang.String"/>
		<parameter name="P_TH_REF_ID" class="java.lang.String"/>
		<parameter name="P_YEAR" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT TH_REF_ID,TH_TRAINING_TITLE,
(
    SELECT UPPER(DECODE(TH_TRAINING_VENUE,NULL,'',TH_TRAINING_VENUE)||DECODE(TH_TRAINING_STATE,NULL,'',', '||STATE_MAIN.SM_STATE_DESC)||
    DECODE(TH_TRAINING_COUNTRY,NULL,'',', '||COUNTRY_MAIN.CM_COUNTRY_DESC)) TH_VENUE
    FROM TRAINING_HEAD,STATE_MAIN,COUNTRY_MAIN
    WHERE TH_TRAINING_STATE = SM_STATE_CODE(+)
    AND TH_TRAINING_COUNTRY = CM_COUNTRY_CODE(+)
    AND TH_REF_ID = $P{P_TH_REF_ID}
) TH_VENUE,
(
    SELECT TO_CHAR(TH_DATE_FROM,'dd-mm-yyyy')||'  hingga '||TO_CHAR(TH_DATE_TO,'dd-mm-yyyy') TARIKH
    FROM TRAINING_HEAD
    WHERE TH_REF_ID = $P{P_TH_REF_ID}
) TARIKH,
(
    SELECT UPPER(DECODE(SG_GROUP_DESC, NULL,'Terbuka',SG_GROUP_DESC)) SG_GROUP_DESC
    FROM TRAINING_HEAD,SERVICE_GROUP
    WHERE SG_GROUP_CODE(+) = TH_SERVICE_GROUP
    AND TH_REF_ID = $P{P_TH_REF_ID}
) SG_GROUP_DESC,
CASE 
WHEN $P{P_STH_STATUS} = 'APPLY'
THEN 'Memohon'
WHEN $P{P_STH_STATUS} = 'VERIFY'
THEN 'Disahkan KJ'
WHEN $P{P_STH_STATUS} = 'RECOMMEND'
THEN 'Diperakukan KJ'
WHEN $P{P_STH_STATUS} = 'APPROVE'
THEN 'Diluluskan'
WHEN $P{P_STH_STATUS} = 'REJECT'
THEN 'Ditolak'
ELSE 'Dibatalkan'
END AS STATUS,
STH_STAFF_ID,TM_TITLE_DESC||' '||SM_STAFF_NAME SM_STAFF_NAME,SM_DEPT_CODE, SS_SERVICE_DESC,SS_SALARY_GRADE,DM_DEPT_DESC,
TO_CHAR(STH_APPLY_DATE,'dd-mm-yyyy') STH_APPLY_DATE,
COALESCE((
    SELECT SCH_JUM_UMUM FROM STAFF_CPD_HEAD
    WHERE SCH_STAFF_ID = STH_STAFF_ID 
    AND SCH_TAHUN = $P{P_YEAR}
), 0) CF_UMUM,
COALESCE((
    SELECT SCH_JUM_KHUSUS FROM STAFF_CPD_HEAD
    WHERE SCH_STAFF_ID = STH_STAFF_ID 
    AND SCH_TAHUN = $P{P_YEAR}
),0) CF_KHUSUS,
COALESCE((
    SELECT SCH_JUM_CPD FROM STAFF_CPD_HEAD
    WHERE SCH_STAFF_ID = STH_STAFF_ID 
    AND SCH_TAHUN = $P{P_YEAR}
),0) CF_JUM
FROM STAFF_TRAINING_HEAD,STAFF_MAIN,TRAINING_HEAD,TRAINING_TYPE,DEPARTMENT_MAIN, SERVICE_SCHEME,TITLE_MAIN
WHERE SM_STAFF_ID = STH_STAFF_ID
AND DM_DEPT_CODE = SM_DEPT_CODE
AND SM_JOB_CODE = SS_SERVICE_CODE
AND SM_STAS_TITLE = TM_TITLE_CODE(+)
AND TH_REF_ID = STH_TRAINING_REFID
AND TT_CODE(+) = TH_TYPE
AND STH_PARTICIPANT_ROLE = 'D'
AND ($P{P_YEAR} IS NULL OR ($P{P_YEAR} IS NOT NULL AND TO_CHAR(TH_DATE_FROM,'yyyy') = $P{P_YEAR}))
AND ($P{P_TH_REF_ID} IS NULL OR ($P{P_TH_REF_ID} IS NOT NULL AND STH_TRAINING_REFID = $P{P_TH_REF_ID}))
AND ($P{P_TH_DATE_FROM} IS NULL OR ($P{P_TH_DATE_FROM} IS NOT NULL AND (TO_CHAR(TH_DATE_FROM,'yyyy') = $P{P_TH_DATE_FROM} OR TO_CHAR(TH_DATE_FROM,'mm-yyyy') = $P{P_TH_DATE_FROM} OR TO_CHAR(TH_DATE_FROM,'dd-mm-yyyy') = $P{P_TH_DATE_FROM})))
AND STH_STAFF_ID IN
(
    SELECT STH_STAFF_ID
    FROM STAFF_TRAINING_HEAD,TRAINING_HEAD
    WHERE $P{P_STH_STATUS} = 'APPROVE'
    AND STH_STATUS = 'APPROVE'
    AND STH_TRAINING_REFID = TH_REF_ID
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = $P{P_TH_REF_ID}
    AND TH_INTERNAL_EXTERNAL <> 'EXTERNAL_AGENCY'
    AND TO_CHAR(TH_DATE_FROM,'yyyy') < '2016'
    UNION
    SELECT STH_STAFF_ID
    FROM STAFF_TRAINING_HEAD,TRAINING_HEAD
    WHERE $P{P_STH_STATUS} = 'APPROVE'
    AND STH_STATUS = 'APPROVE'
    AND STH_TRAINING_REFID = TH_REF_ID
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = $P{P_TH_REF_ID}
    AND TH_INTERNAL_EXTERNAL = 'EXTERNAL_AGENCY'
    UNION
    SELECT STD_STAFF_ID 
    FROM STAFF_TRAINING_DETL,TRAINING_HEAD,STAFF_TRAINING_HEAD
    WHERE COALESCE(STD_ATTEND,'N') IN ('Y','A')
    AND STD_TRAINING_REFID = $P{P_TH_REF_ID}
    AND $P{P_STH_STATUS} = 'APPROVE'
    AND STH_STATUS = 'APPROVE'
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = STD_TRAINING_REFID
    AND STH_TRAINING_REFID = TH_REF_ID
    AND TH_INTERNAL_EXTERNAL <> 'EXTERNAL_AGENCY'
    AND TO_CHAR(TH_DATE_FROM,'yyyy') >= '2016'
    UNION
    SELECT STH_STAFF_ID
    FROM STAFF_TRAINING_HEAD,TRAINING_HEAD
    WHERE $P{P_STH_STATUS} = 'REJECT'
    AND STH_STATUS = 'REJECT'
    AND STH_TRAINING_REFID = TH_REF_ID
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = $P{P_TH_REF_ID}
    AND TO_CHAR(TH_DATE_FROM,'yyyy') < '2016'
    AND TH_INTERNAL_EXTERNAL <> 'EXTERNAL_AGENCY'
    UNION
    SELECT STH_STAFF_ID
    FROM STAFF_TRAINING_HEAD,TRAINING_HEAD
    WHERE $P{P_STH_STATUS} = 'REJECT'
    AND STH_STATUS = 'REJECT'
    AND STH_TRAINING_REFID = TH_REF_ID
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = $P{P_TH_REF_ID}
    AND TH_INTERNAL_EXTERNAL = 'EXTERNAL_AGENCY'
    UNION
    SELECT STH_STAFF_ID
    FROM STAFF_TRAINING_HEAD,TRAINING_HEAD
    WHERE $P{P_STH_STATUS} = 'REJECT'
    AND STH_STATUS = 'REJECT'
    AND STH_TRAINING_REFID = TH_REF_ID
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = $P{P_TH_REF_ID}
    AND TH_INTERNAL_EXTERNAL <> 'EXTERNAL_AGENCY'
    AND TO_CHAR(TH_DATE_FROM,'yyyy') >= '2016'
    UNION
    SELECT STD_STAFF_ID 
    FROM STAFF_TRAINING_DETL
    WHERE STD_ATTEND = 'N'
    AND $P{P_STH_STATUS} = 'REJECT'
    AND STD_TRAINING_REFID = $P{P_TH_REF_ID}
    AND STD_STAFF_ID IN
     (
      SELECT STH_STAFF_ID 
      FROM STAFF_TRAINING_HEAD,TRAINING_HEAD
      WHERE STH_STATUS = 'APPROVE' 
      AND STH_PARTICIPANT_ROLE = 'D'
      AND STH_TRAINING_REFID = TH_REF_ID
      AND TH_INTERNAL_EXTERNAL <> 'EXTERNAL_AGENCY'
      AND TO_CHAR(TH_DATE_FROM,'yyyy') >= '2016'
      )
    UNION
    SELECT STH_STAFF_ID
    FROM STAFF_TRAINING_HEAD
    WHERE STH_STATUS = 'CANCEL'
    AND $P{P_STH_STATUS} = 'CANCEL'
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = $P{P_TH_REF_ID}
    UNION
    SELECT STH_STAFF_ID
    FROM STAFF_TRAINING_HEAD
    WHERE STH_STATUS IN ('RECOMMEND','RECOMMEND_BSM')
    AND $P{P_STH_STATUS} = 'RECOMMEND'
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = $P{P_TH_REF_ID}
    UNION
    SELECT STH_STAFF_ID
    FROM STAFF_TRAINING_HEAD
    WHERE STH_STATUS = 'VERIFY'
    AND $P{P_STH_STATUS} = 'VERIFY'
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = $P{P_TH_REF_ID}
    UNION
    SELECT STH_STAFF_ID
    FROM STAFF_TRAINING_HEAD
    WHERE STH_STATUS = 'APPLY'
    AND $P{P_STH_STATUS} = 'APPLY'
    AND STH_PARTICIPANT_ROLE = 'D'
    AND STH_TRAINING_REFID = $P{P_TH_REF_ID}
)
ORDER BY DECODE(LENGTH(SS_SALARY_GRADE),2,SUBSTR(SS_SALARY_GRADE,2,1),3,SUBSTR(SS_SALARY_GRADE,2,2),4,SUBSTR(SS_SALARY_GRADE,3,2),5,SUBSTR(SS_SALARY_GRADE,4,2)) DESC,SS_SERVICE_DESC,DM_DEPT_DESC,STH_STAFF_ID]]>
		</queryString>
