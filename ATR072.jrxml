END),STH_STAFF_ID
HAVING SUM(TH_DATE_TO-TH_DATE_FROM+1) < 7
)
GROUP BY SS_SERVICE_KUMPP3
) ON SS_SERVICE_KUMPP1 = SS_SERVICE_KUMPP3
ORDER BY SS_SERVICE_KUMPP1]]>
		</queryString>
