	<subDataset name="INDEX_A1" uuid="857849ab-bf2c-4f13-8c0b-b73be260d80a">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MyHRIS"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="STAFF_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
CASE
WHEN CM_YEAR = TO_CHAR(SYSDATE,'yyyy')
    THEN ' (i) '
    ELSE ' (ii) '
END AS "CF_INDEX_A",
CASE
WHEN CM_YEAR = TO_CHAR(SYSDATE,'yyyy')
    THEN 'Tahun ini (' ||	CM_YEAR || ') ' || chr(32) ||  chr(32) || chr(32) ||  chr(32) || CS_CTR_CON || ' kali'
    ELSE 'Tahun Lepas (' ||	CM_YEAR || ') ' || chr(32) ||  chr(32) || chr(32) ||  chr(32) || CS_CTR_CON || ' kali'
END AS "CF_INDEX_A"
FROM
(
    SELECT
    COUNT(SCM_STAFF_ID) CS_CTR_CON,
    CM_YEAR
    FROM
    (
        SELECT TO_CHAR(CM_DATE_FROM,'yyyy') AS "CM_YEAR",
        SCM_REFID AS "SCM_REFID",
        SCM_STAFF_ID AS "SCM_STAFF_ID",
        CM_NAME AS "CM_NAME",
        CM_ADDRESS AS "CM_ADDRESS",
        CM_CITY AS "CM_CITY",
        CM_POSTCODE AS "CM_POSTCODE",
        CM_STATE AS "CM_STATE",
        CONFERENCE_MAIN.CM_COUNTRY_CODE AS "CM_COUNTRY_CODE",
        SM_STATE_DESC AS "SM_STATE_DESC",
        CM_COUNTRY_DESC AS "CM_COUNTRY_DESC",
        CM_DATE_FROM AS "CM_DATE_FROM",
        CM_DATE_TO AS "CM_DATE_TO"    
        FROM STAFF_CONFERENCE_MAIN,CONFERENCE_MAIN,STATE_MAIN,COUNTRY_MAIN
        WHERE CM_REFID = SCM_REFID
        AND COUNTRY_MAIN.CM_COUNTRY_CODE(+) = CONFERENCE_MAIN.CM_COUNTRY_CODE
        AND SM_STATE_CODE(+) = CM_STATE
        AND COALESCE(CONFERENCE_MAIN.CM_COUNTRY_CODE,'MYS') <> 'MYS'
        AND SCM_STAFF_ID = $P{STAFF_ID}
        AND
        (TO_CHAR(CM_DATE_FROM,'yyyy') = TO_CHAR(SYSDATE,'yyyy')
        OR TO_CHAR(CM_DATE_FROM,'yyyy') = TO_CHAR(SYSDATE-365.25,'yyyy'))
        ORDER BY CM_YEAR DESC,CM_DATE_FROM,CM_DATE_TO
    )
    GROUP BY CM_YEAR
)]]>
		</queryString>
		<field name="CF_INDEX_A" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="CF_INDEX_A"/>
		</field>
