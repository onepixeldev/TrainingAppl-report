	<queryString>
		<![CDATA[SELECT
E.*,
CASE
WHEN LINE3 IS NOT NULL AND OUTPUT2 IS NOT NULL
        THEN OUTPUT2 || CHR(10) || LINE3
WHEN LINE3 IS NOT NULL AND OUTPUT2 IS NULL
    THEN OUTPUT2 || LINE3
END AS "OUTPUT3"
FROM
(
    SELECT
    W.*,
    CASE 
    WHEN SCA_DATE_FROM IS NOT NULL AND SCA_DATE_TO IS NOT NULL 
        THEN (initcap(calendar.CONVERT_TO_MALAY_DATE(to_char(W.SCA_DATE_FROM,'dd-mm-yyyy'),'DD MONTH YYYY')
            || ' - ' || calendar.CONVERT_TO_MALAY_DATE(to_char(W.SCA_DATE_TO,'dd-mm-yyyy'),'DD MONTH YYYY')) 
            || ' ( ' || W.SCA_DURATION || ' hari ) ')
        ELSE (initcap(calendar.CONVERT_TO_MALAY_DATE(to_char(W.CM_DATE_FROM,'dd-mm-yyyy'),'DD MONTH YYYY')
            || ' - ' || calendar.CONVERT_TO_MALAY_DATE(to_char(W.CM_DATE_TO,'dd-mm-yyyy'),'DD MONTH YYYY')) 
            || ' ( ' || cm_duration || ' hari ) ')     
    END AS "CF_DATE", 
    CASE 
    WHEN LINE2 IS NOT NULL AND LINE1 IS NOT NULL
        THEN LINE1 || CHR(10) || LINE2
    WHEN LINE2 IS NOT NULL AND LINE1 IS NULL
        THEN LINE1 || LINE2
    END AS "OUTPUT2"
    FROM
    (
        SELECT
        Q.*,
        CASE 
        WHEN CM_ADDRESS IS NOT NULL
            THEN CM_ADDRESS
            ELSE ''
        END AS "LINE1",    
        CASE 
        WHEN CM_POSTCODE IS NOT NULL AND CM_CITY IS NOT NULL
           THEN CM_POSTCODE|| ', ' ||CM_CITY
        WHEN CM_POSTCODE IS NOT NULL AND CM_CITY IS NULL
            THEN CM_POSTCODE
        WHEN CM_POSTCODE IS NULL AND CM_CITY IS NOT NULL   
            THEN CM_CITY
        WHEN CM_POSTCODE IS NULL AND CM_CITY IS NULL 
            THEN ''
        END AS "LINE2",
        CASE 
        WHEN SM_STATE_DESC IS NOT NULL AND CM_COUNTRY_DESC IS NOT NULL
           THEN SM_STATE_DESC|| ', ' ||CM_COUNTRY_DESC
        WHEN SM_STATE_DESC IS NOT NULL AND CM_COUNTRY_DESC IS NULL
            THEN SM_STATE_DESC
        WHEN SM_STATE_DESC IS NULL AND CM_COUNTRY_DESC IS NOT NULL
            THEN CM_COUNTRY_DESC
        WHEN SM_STATE_DESC IS NULL AND CM_COUNTRY_DESC IS NULL 
            THEN ''
        END AS "LINE3",
        (SELECT Q.CM_DATE_TO-Q.CM_DATE_FROM+1 FROM DUAL) AS "CM_DURATION"
        FROM
        (  
            SELECT 
            SCA_REFID AS "SCA_REFID",
            SCA_STAFF_ID AS "SCA_STAFF_ID",
            CM_NAME AS "CM_NAME",
            SCA_REASON AS "SCA_REASON",
            CM_ADDRESS AS "CM_ADDRESS",
            CM_CITY AS "CM_CITY",
            CM_POSTCODE AS "CM_POSTCODE",
            CM_STATE AS "CM_STATE",
            CM.CM_COUNTRY_CODE AS "CM_COUNTRY_CODE2",
            SM_STATE_DESC AS "SM_STATE_DESC",
            CM_COUNTRY_DESC AS "CM_COUNTRY_DESC",
            CM_DATE_FROM AS "CM_DATE_FROM",
            CM_DATE_TO AS "CM_DATE_TO",
            SCA_PARTICIPANT_INFO AS "SCA_PARTICIPANT_INFO",
            (COALESCE(SCA_EMBASSY_INFO,'-')) AS "SCA_EMBASSY_INFO",
            SCA_FREQUENCY AS "SCA_FREQUENCY",
            SCA_SPONSOR_BY AS "SCA_SPONSOR_BY",
            TO_CHAR(SCA_SPONSOR_RM, '999,999,999,999.99') AS "SCA_SPONSOR_RM",
            SCA_BENEFIT_TO_COUNTRY AS "SCA_BENEFIT_TO_COUNTRY",
            (COALESCE(SCA_MINISTRY_APPROVAL,'-')) AS "SCA_MINISTRY_APPROVAL",
            SCAP_NAME AS "SCAP_NAME",
            SCAP_POSITION AS "SCAP_POSITION",
            SM_STAFF_NAME AS "SM_STAFF_NAME",
            SS_DESC_SHORT AS "SS_DESC_SHORT",
            SCA_DATE_FROM AS "SCA_DATE_FROM",
            SCA_DATE_TO AS "SCA_DATE_TO",
            (SCA_DATE_TO-SCA_DATE_FROM+1) AS "SCA_DURATION",
            (
                SELECT DM1.DM_DEPT_DESC||DECODE(DM2.DM_DEPT_DESC,NULL,'',' / '||DM2.DM_DEPT_DESC)
                FROM STAFF_MAIN,DEPARTMENT_MAIN DM1,DEPARTMENT_MAIN DM2
                WHERE SM_DEPT_CODE = DM1.DM_DEPT_CODE
                AND SM_UNIT(+) = DM2.DM_DEPT_CODE
                AND SM_STAFF_ID = SCA_STAFF_ID
            ) AS "CF_SM_DEPT"
            FROM STAFF_CONFERENCE_A,STAFF_CONFERENCE_A_PART,
            CONFERENCE_MAIN CM,STATE_MAIN,COUNTRY_MAIN CM2,
            STAFF_MAIN,SERVICE_SCHEME
            WHERE CM_REFID = SCA_REFID
            AND SM_STAFF_ID = SCA_STAFF_ID
            AND SS_SERVICE_CODE(+) = SM_JOB_CODE
            AND CM2.CM_COUNTRY_CODE = CM.CM_COUNTRY_CODE
            AND SM_STATE_CODE = CM_STATE
            AND SCAP_REFID(+) = SCA_REFID
            AND SCAP_STAFF_ID(+) = SCA_STAFF_ID
            AND SCA_REFID = $P{CONFERENCE_ID}
            AND SCA_STAFF_ID = $P{STAFF_ID}
        )Q
    )W
)E]]>
	</queryString>
