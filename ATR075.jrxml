		<queryString>
			<![CDATA[SELECT
E.*,
CASE
WHEN LINE3 IS NOT NULL AND OUTPUT2 IS NOT NULL
    THEN OUTPUT2 || ', ' || LINE3
WHEN LINE3 IS NOT NULL AND OUTPUT2 IS NULL  
    THEN OUTPUT2 || LINE3
END AS "OUTPUT3"    
FROM
(
    SELECT
    W.*,
    CASE 
    WHEN LINE2 IS NOT NULL AND LINE1 IS NOT NULL
        THEN LINE1 || ', ' || LINE2
    WHEN LINE2 IS NOT NULL AND LINE1 IS NULL  
        THEN LINE1 || LINE2
    END AS "OUTPUT2"    
    FROM
    (
        SELECT
        Q.*,
        CASE
        WHEN CM_ADDRESS IS NOT NULL
            THEN CM_ADDRESS
        END AS "LINE1" ,
        CASE
        WHEN CM_POSTCODE IS NOT NULL AND CM_CITY IS NOT NULL
            THEN CM_POSTCODE || ', ' || CM_CITY
        WHEN CM_POSTCODE IS NOT NULL AND CM_CITY IS NULL
            THEN CM_POSTCODE   
        WHEN CM_POSTCODE IS NULL AND CM_CITY IS NOT NULL
            THEN CM_CITY
            ELSE ''
        END AS "LINE2",
        CASE
        WHEN SM_STATE_DESC IS NOT NULL AND CM_COUNTRY_DESC IS NOT NULL AND CM_STATE <> '98'
            THEN SM_STATE_DESC || ', ' || CM_COUNTRY_DESC
        WHEN SM_STATE_DESC IS NOT NULL AND CM_COUNTRY_DESC IS NULL AND CM_STATE <> '98'
            THEN SM_STATE_DESC   
        WHEN SM_STATE_DESC IS NULL AND CM_COUNTRY_DESC IS NOT NULL AND CM_STATE <> '98'
            THEN CM_COUNTRY_DESC
        WHEN SM_STATE_DESC IS NULL AND CM_COUNTRY_DESC IS NULL
            THEN ''
            ELSE CM_COUNTRY_DESC
        END AS "LINE3"
        FROM
        (
            SELECT TO_CHAR(CM_DATE_FROM,'yyyy') AS "CM_YEAR",
            SCM_REFID AS "SCM_REFID",
            SCM_STAFF_ID AS "SCM_STAFF_ID",
            CM_NAME AS "CM_NAME",
            CM_ADDRESS AS "CM_ADDRESS",
            CM_CITY AS "CM_CITY",
            CM_POSTCODE AS "CM_POSTCODE",
            CM_STATE AS "CM_STATE",
            CONFERENCE_MAIN.CM_COUNTRY_CODE AS "CM_COUNTRY_CODE",
            SM_STATE_DESC AS "SM_STATE_DESC",
            CM_COUNTRY_DESC AS "CM_COUNTRY_DESC",
            CM_DATE_FROM AS "CM_DATE_FROM",
            CM_DATE_TO AS "CM_DATE_TO",
            (
                SELECT SCA_REASON FROM STAFF_CONFERENCE_A
                WHERE SCA_REFID = SCM_REFID
                AND SCA_STAFF_ID = SCM_STAFF_ID
            ) AS "CF_REASON",
            INITCAP(CALENDAR.CONVERT_TO_MALAY_DATE(TO_CHAR(CM_DATE_FROM,'dd-mm-yyyy'),'DD MONTH YYYY')
            || ' - ' || CALENDAR.CONVERT_TO_MALAY_DATE(TO_CHAR(CM_DATE_TO,'dd-mm-yyyy'),'DD MONTH YYYY')) AS CF_DATE_A1
            FROM STAFF_CONFERENCE_MAIN,CONFERENCE_MAIN,STATE_MAIN,COUNTRY_MAIN
            WHERE CM_REFID = SCM_REFID
            AND COUNTRY_MAIN.CM_COUNTRY_CODE(+) = CONFERENCE_MAIN.CM_COUNTRY_CODE
            AND SM_STATE_CODE(+) = CM_STATE
            AND COALESCE(CONFERENCE_MAIN.CM_COUNTRY_CODE,'MYS') <> 'MYS'
            AND SCM_STAFF_ID = $P{STAFF_ID}
            AND
            (TO_CHAR(CM_DATE_FROM,'yyyy') = TO_CHAR(SYSDATE,'yyyy')
            OR TO_CHAR(CM_DATE_FROM,'yyyy') = TO_CHAR(SYSDATE-365.25,'yyyy'))
            ORDER BY CM_YEAR DESC,CM_DATE_FROM,CM_DATE_TO
        )Q
    )W
)E]]>
		</queryString>
