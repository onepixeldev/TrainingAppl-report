		<parameter name="P_OPTION" class="java.lang.String"/>
		<parameter name="P_PTJ" class="java.lang.String"/>
		<parameter name="TRAINING_MONTH" class="java.lang.String"/>
		<parameter name="TRAINING_MONTH2" class="java.lang.String"/>
		<parameter name="TRAINING_YEAR" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT 
BH_REF_ID,
BHTITLE, 
BH_TITLE,
BH_DATE,
TSL_DESC,
COORDINATOR,
BH_VENUE,
BH_PARTICIPANT,
SB_TOTAL,
ROUND((SB_TOTAL / BH_PARTICIPANT) * 100,0) CF_PERATUS_BITARA,
CF_THADIR,
ROUND((CF_THADIR / BH_PARTICIPANT) * 100,0) CF_PERATUS_THADIR
FROM
(
    SELECT
    BHTITLE,
    BH_REF_ID,
    BH_TITLE,
    BH_DATE,
    BH_VENUE,
    BH_DATE_FROM,
    SB_TOTAL,
    BH_PARTICIPANT,
    (BH_PARTICIPANT - SB_TOTAL) CF_THADIR
    FROM
    (
        SELECT 'Program Bitara UPSI' BHTITLE,BH_REF_ID,BH_TITLE,
        TO_CHAR(BH_DATE_FROM,'dd/mm/yyyy')||' - '||TO_CHAR(BH_END_DATE,'dd/mm/yyyy') BH_DATE,
        BH_VENUE,BH_DATE_FROM,COUNT(SB_STAFF_ID) SB_TOTAL,BH_PARTICIPANT 
        FROM BITARA_HEAD,STAFF_BITARA
        WHERE BH_REF_ID=SB_REF_ID
        AND SB_STATUS='APPROVED'
        AND SB_ATTENDANCE='Y'
        AND TO_CHAR(BH_DATE_FROM,'yyyy')=$P{TRAINING_YEAR}
        AND (BH_DATE_FROM BETWEEN TO_DATE($P{TRAINING_MONTH},'mm/yyyy')
        AND LAST_DAY(TO_DATE($P{TRAINING_MONTH2},'mm/yyyy')))
        AND ($P{P_OPTION} IS NULL OR ($P{P_OPTION} IS NOT NULL AND $P{P_OPTION}='INTERNAL'))
        AND ($P{P_PTJ} IS NULL OR ($P{P_PTJ} IS NOT NULL AND $P{P_PTJ} = $P{P_PTJ}))
        GROUP BY 'Program Bitara UPSI',BH_REF_ID,BH_TITLE,
        TO_CHAR(BH_DATE_FROM,'dd/mm/yyyy')||' - '||TO_CHAR(BH_END_DATE,'dd/mm/yyyy'),
        BH_VENUE,BH_DATE_FROM,BH_PARTICIPANT
        ORDER BY BH_DATE_FROM
    )
)
LEFT JOIN
(
    SELECT TSL_DESC, THD_REF_ID THD_REF_ID1
    FROM TRAINING_SECTOR_LEVEL,TRAINING_HEAD_DETL 
    WHERE TSL_CODE = THD_COORDINATOR_SECTOR
) ON THD_REF_ID1 = BH_REF_ID
LEFT JOIN
(
    SELECT SM_STAFF_NAME||' ('||THD_COORDINATOR||')' AS COORDINATOR, THD_REF_ID THD_REF_ID2
    FROM STAFF_MAIN,TRAINING_HEAD_DETL
    WHERE SM_STAFF_ID = THD_COORDINATOR
) ON THD_REF_ID2 = BH_REF_ID]]>
		</queryString>
