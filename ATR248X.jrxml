		</field>
		<field name="AGE" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="AGE"/>
		</field>
		<field name="RM_RACE_DESC" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="RM_RACE_DESC"/>
		</field>
		<field name="KEHADIRAN" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="KEHADIRAN"/>
