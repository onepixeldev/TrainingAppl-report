	<subDataset name="STAFF_LIST" uuid="4a353e74-bb26-421b-88f5-23635040c3d9">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MyHRIS"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="AKA_NAKA" class="java.lang.String"/>
		<parameter name="TH_REFID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
STH_TRAINING_REFID,
STH_STAFF_ID,
CF_TITLE,
CF_ADMIN_POST,
SM_DEPT_CODE,
CF_SERVICE_KUMP,
SS_SERVICE_DESC,
SM_GENDER,
AGE,
RM_RACE_DESC,
CASE 
WHEN TOT_STAFF_ATTEND > 0 AND TOT_STAFF_ATTEND2 < 1
THEN 'Y'
WHEN TOT_STAFF_ATTEND < 1 AND TOT_STAFF_ATTEND2 > 0
THEN 'N'
END AS KEHADIRAN
FROM
(
    SELECT STH_TRAINING_REFID,
    STH_STAFF_ID,
    CASE WHEN GET_STAFF_TITLE(STH_STAFF_ID) IS NULL
    THEN STAFF_NAME
    ELSE GET_STAFF_TITLE(STH_STAFF_ID)||' '||STAFF_NAME
    END AS CF_TITLE,
    (
        SELECT INITCAP(APM_DESC)||chr(10)||' ('||SM_ADMIN_DEPT||')' ADMIN_DEPT
        FROM STAFF_MAIN,ADMIN_POST_MAIN
        WHERE SM_ADMIN_JOBCODE = APM_CODE
        AND SM_STAFF_ID = Q.STH_STAFF_ID
    ) CF_ADMIN_POST,
    SM_DEPT_CODE,
    (
        SELECT SOG_GROUP_DESC||CHR(10)||'('||COALESCE(SS_SERVICE_KUMPP,'Tiada Kumpulan Perkhidmatan')||')' KUMPULAN_PERKHIDMATAN
        FROM SERVICE_SCHEME,STAFF_MAIN,SERVICE_ORG_GROUP
        WHERE SM_JOB_CODE = SS_SERVICE_CODE
        AND SM_STAFF_ID = Q.STH_STAFF_ID
        AND SS_SERVICE_KUMPP = SOG_GROUP_CODE
    ) CF_SERVICE_KUMP,
    SS_SERVICE_DESC,
    SM_GENDER,
    STAFF.GET_AGE(Q.TH_DATE_FROM,Q.SM_BIRTH_DATE) AGE,
    RM_RACE_DESC,
    (
        SELECT COUNT(STH_STAFF_ID) AS TOT_STAFF_ATTEND
        FROM TRAINING_HEAD, STAFF_TRAINING_HEAD
        WHERE TH_REF_ID = STH_TRAINING_REFID
        AND TH_STATUS = 'APPROVE'
        AND STH_STATUS = 'APPROVE'
        AND 
        (
            (
              TO_CHAR(TH_DATE_FROM,'yyyy') < '2016'
              AND TH_INTERNAL_EXTERNAL <> 'EXTERNAL_AGENCY'
              OR
              (
              TH_INTERNAL_EXTERNAL = 'EXTERNAL_AGENCY'
              )
              OR
              (
                TO_CHAR(TH_DATE_FROM,'yyyy') >= '2016'
                AND EXISTS (
                  SELECT (1) FROM STAFF_TRAINING_DETL
                  WHERE STD_TRAINING_REFID = STH_TRAINING_REFID
                  AND STD_STAFF_ID = STH_STAFF_ID
                  AND COALESCE(STD_ATTEND,'N') IN ('Y','A')
                )
            )
          )
        )
        AND STH_STAFF_ID = Q.STH_STAFF_ID
        AND TH_REF_ID = Q.STH_TRAINING_REFID
    ) TOT_STAFF_ATTEND,
    (
        SELECT COUNT(STH_STAFF_ID) AS TOT_STAFF_ATTEND
        FROM TRAINING_HEAD, STAFF_TRAINING_HEAD
        WHERE TH_REF_ID = STH_TRAINING_REFID
        AND TH_STATUS = 'APPROVE'
        AND 
        (
          (
              (
                TO_CHAR(TH_DATE_FROM,'yyyy') < '2016'
                AND STH_STATUS NOT IN ('APPROVE','CANCEL')
                AND TH_INTERNAL_EXTERNAL <> 'EXTERNAL_AGENCY'
              )
              OR
              (
                  
                STH_STATUS NOT IN ('APPROVE','CANCEL') 
                AND TH_INTERNAL_EXTERNAL = 'EXTERNAL_AGENCY'
              )
              OR 
              (
                --TO_CHAR(TH_DATE_FROM,'yyyy') >= '2016'
                EXISTS 
                (
                  SELECT (1) FROM STAFF_TRAINING_DETL
                  WHERE STD_TRAINING_REFID = STH_TRAINING_REFID
                  AND STD_STAFF_ID = STH_STAFF_ID
                  AND STD_ATTEND = 'N'
                  AND STD_STAFF_ID IN
                  (
                        SELECT STH_STAFF_ID 
                        FROM STAFF_TRAINING_HEAD,TRAINING_HEAD
                        WHERE STH_STATUS = 'APPROVE' 
                        AND STH_TRAINING_REFID = TH_REF_ID
                        AND TH_INTERNAL_EXTERNAL <> 'EXTERNAL_AGENCY'
                        AND TO_CHAR(TH_DATE_FROM,'yyyy') >= '2016'
                  )
                )
              )
          )
        )
        AND STH_STAFF_ID = Q.STH_STAFF_ID
        AND TH_REF_ID = Q.STH_TRAINING_REFID
    ) TOT_STAFF_ATTEND2
    FROM
    (
        SELECT STH_TRAINING_REFID,STH_STAFF_ID,STAFF_NAME,SM_DEPT_CODE,SS_SERVICE_DESC,SM_GENDER,RM_RACE_DESC,SM_BIRTH_DATE,TH_DATE_FROM
        FROM
        (
            SELECT STH_TRAINING_REFID,STH_STAFF_ID,SM_STAFF_NAME||CHR(10)||'('||STH_STAFF_ID||')' STAFF_NAME,SM_DEPT_CODE,SS_SERVICE_DESC,SM_GENDER,RM_RACE_DESC,
            SM_BIRTH_DATE,TH_DATE_FROM
            FROM STAFF_TRAINING_HEAD,STAFF_MAIN,SERVICE_SCHEME,RACE_MAIN,TRAINING_HEAD
            WHERE STH_TRAINING_REFID = TH_REF_ID
            AND TO_CHAR(TH_DATE_FROM,'yyyy') < '2016'
            AND SM_STAFF_ID = STH_STAFF_ID
            AND SM_JOB_CODE = SS_SERVICE_CODE
            AND ($P{AKA_NAKA} IS NULL OR ($P{AKA_NAKA} IS NOT NULL AND COALESCE(SS_ACADEMIC,'N') = $P{AKA_NAKA}))
            AND SM_RACE = RM_RACE_CODE
            AND STH_STATUS <> 'CANCEL'
            UNION
            SELECT STH_TRAINING_REFID,STH_STAFF_ID,SM_STAFF_NAME||CHR(10)||'('||STH_STAFF_ID||')' STAFF_NAME,SM_DEPT_CODE,SS_SERVICE_DESC,SM_GENDER,RM_RACE_DESC,
            SM_BIRTH_DATE,TH_DATE_FROM
            FROM STAFF_TRAINING_HEAD,STAFF_MAIN,SERVICE_SCHEME,RACE_MAIN,TRAINING_HEAD
            WHERE STH_TRAINING_REFID = TH_REF_ID
            AND TO_CHAR(TH_DATE_FROM,'yyyy') >= '2016'
            AND SM_STAFF_ID = STH_STAFF_ID
            AND SM_JOB_CODE = SS_SERVICE_CODE
            AND STH_STATUS = 'APPROVE'
            AND ($P{AKA_NAKA} IS NULL OR ($P{AKA_NAKA} IS NOT NULL AND COALESCE(SS_ACADEMIC,'N') = $P{AKA_NAKA}))
            AND SM_RACE = RM_RACE_CODE
        )
    ) Q
    WHERE STH_TRAINING_REFID = $P{TH_REFID}
    ORDER BY STH_STAFF_ID
) W]]>
		</queryString>
